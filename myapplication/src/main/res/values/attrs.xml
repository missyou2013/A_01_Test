<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Dialog">
        <attr name="DialogTitleAppearance" format="reference" />
        <attr name="DialogTitleText" format="reference|string" />
        <attr name="DialogSpotColor" format="reference|color"/>
        <attr name="DialogSpotCount" format="integer"/>
    </declare-styleable>
    <!-- CircleProgressBar-->
    <declare-styleable name="TasksCompletedView">
        <attr name="radius" format="dimension"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="circleColor" format="color"/>
        <attr name="ringColor" format="color"/>
        <attr name="secondRingColor" format="color"/>
        <attr name="ringDistance" format="dimension"/>
        <attr name="progress" format="integer"/>
        <attr name="startAngle" format="integer" />
        <attr name="sweepAngle" format="integer" />
    </declare-styleable>
    <!-- CircleImageView -->
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <!-- FadeImageButton-->
    <declare-styleable name="FadeImageButton">
        <attr name="top_image_src" format="reference"/>
        <attr name="bottom_image_src" format="reference"/>
    </declare-styleable>
</resources>