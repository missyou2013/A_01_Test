<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ac.test.liu.com.liuapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.test.liu.application.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 首页 -->
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 测试 -->
        <activity android:name=".TestActivity" />
        <activity android:name=".A000Activity" />
        <activity android:name=".tablayout.TablayoutActivity"></activity>
        <activity android:name=".tablayout.Tablayout2Activity"></activity>
        <activity android:name=".tablayout.Tablayout3Activity"></activity>

        <!-- 登录动画 -->
        <activity
            android:name="ac.test.liu.com.liuapplication.readme.io.LoginActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="ac.test.liu.com.liuapplication.readme.io.RegisterActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name="ac.test.liu.com.liuapplication.readme.io.MainActivity"
            android:screenOrientation="portrait"></activity>

        <!-- RxJava -->
        <activity
            android:name="ac.test.liu.com.liuapplication.rxjava.RxJavaRetrofit2Activity"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name="ac.test.liu.com.liuapplication.rxjava.RxJavaRetrofit2Activity_02"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name="ac.test.liu.com.liuapplication.rxjava.RxJavaRetrofit2Activity_03"
            android:screenOrientation="portrait"></activity>


        <activity
            android:name="ac.test.liu.com.liuapplication.rxjava.rxjavaretrofitdemo.activity.MainTestActivity"
            android:label="rxjava_refoit"
            android:screenOrientation="portrait"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


    </application>

</manifest>